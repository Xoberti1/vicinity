<!-- LOGIN SECTION =======-->
<div id="login-page" class="row">
  <div class="col s12 m6 l3 z-depth-4 card-panel grey lighten-3">
    <form class="login-form">
      <div class="row">

        <div class="input-field col s12 center">
          <img src="../public/assets/images/Logo.png" alt="vicinity" class="logo" id="loglogo">

          <p class="center login-form-text"></p>
        </div>
      </div>

      <div class="row margin">
        <div class="input-field col s12">
          <i class="material-icons prefix">person</i>
          <input id="email" type="email" class="validate">
          <label for="email" class="center-align">Email</label>
        </div>
      </div>

      <div class="row margin">
        <div class="input-field col s12">
          <i class="small material-icons prefix">lock_outline</i>
          <input id="password" type="password" class="validate">
          <label for="password">Password</label>
        </div>
      </div>

      <div class="row">
        <div class="input-field col s12">
          <a href="home.html" class="btn waves-effect waves-light col s12 lime">Login</a>
        </div>
      </div>
      <div class="row">
        <div class="input-field col s6 m6 l6">
          <p class="margin medium-small">
            <a href="page-register.html">Register Now!</a>
          </p>
        </div>
        <div class="input-field col s6 m6 l6">
          <p class="margin right-align medium-small">
            <a href="page-forgot-password.html">Forgot password ?</a>
          </p>
        </div>
      </div>

    </form>
  </div>

  <div class="slogan">
    <h1 class="heading-primary">
      <span class="heading-primary-main">Your neighborhood</span>
      <br>
      <span class="heading-primary sub">Our commitment</span>
    </h1>
  </div>
</div>
