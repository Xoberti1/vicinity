<main>
<br>
<br>
<!-- Profile Card -->
<div class="container">
  <div class="row">
    <section class="col s3">
      <div class="card" id="profile">
        <div>
          <h3 class="card-title">My Profile</h3>
        </div>
        <div class="card-image">
          <img src="images/team2.jpg">
        </div>
        <div class="card-content">
          <p>
            Street Address: {{streetAddress}}
            <br> City: {{city}}
            <br> State: {{state}}
            <br> Zips: {{zip}}
            <br>
          </p>
        </div>
        <div class="card-action">
          <a href="#">Change Address</a>
        </div>
      </div>

      <!--SUBMIT BUTTON======================================================-->
      <button id="reportCrime" class="btn waves-effect waves-light blue-grey" type="submit" name="action">
        Report Crime
      </button>

            <!--SUBMIT BUTTON======================================================-->
      <button id="searchCrime" class="btn waves-effect waves-light blue-grey" type="submit" name="action">
        Search An Area for Crime
      </button>

    </section>
    <section class="col s8 offset-s1" id="resultsTable">
      <div id="crimeRate" class='mytable'>
        <div class="crimeTableHead">
          <h5 class="crimeHeader" style="text-align:center;">Crime Rate</h5>
        </div>
        <div class="crimeTableBody">
          <table class="responsive-table highlight">
            <thead>
              <tr>
                <th>Crime</th>
                <th>Location</th>
                <th>Description</th>
                <th>Date</th>
                <th>Time</th>
              </tr>
            </thead>
            <tr>
              <td>Assault</td>
              <td>10694 Jones Rd</td>
              <td>Threw Computers out Window bc Project 2</td>
              <td>1/25/2018</td>
              <td>11:59 PM</td>
            </tr>
            {{!-- {{#each crime}} {{#if zipCode === zipCode}}
            <tr>
              <td>{{Type}}</td>
              <td>{{Address}}</td>
              <td>{{Date}}</td>
              <td>{{Description}}</td>
              <td>{{Time}}</td>
            </tr>
            {{/if}} {{/each}} --}}
            </tbody>
          </table>
        </div>
      </div>
      <br>
      <br>
      <br>
    </section>
  </div>
</div>

</div>
</main>
