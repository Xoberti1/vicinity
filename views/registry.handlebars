<div id="registry_page" class="row">
    <div class="col s12 m8 l6 z-depth-4 card-panel grey lighten-3 center-align">
        <form class="login-form">
            <div class="row">

                <div class="input-field col s12 center">
                    <img src="./assets/images/Logo.png" alt="vicinity" class="logo" id="loglogo">

                    <p class="center login-form-text"></p>
                </div>
            </div>

            <div class="row margin">
                <div class="input-field col s12">
                    <input id="name" type="text" class="validate">
                    <label for="name" class="center-align">First & Last Name</label>
                </div>
                <div class="input-field col s12">
                    <input id="street" type="text" class="validate">
                    <label for="street" class="center-align">Street Address</label>
                </div>
                <div class="input-field col s12">
                    <input id="city" type="text" class="validate">
                    <label for="city" class="center-align">City</label>
                </div>
                <div class="input-field col s12">
                    <input id="zip" type="text" class="validate">
                    <label for="zip" class="center-align">Zip Code</label>
                </div>
                <div class="input-field col s12">
                    <input id="user_name" type="text" class="validate">
                    <label for="user_name" class="center-align">User Name</label>
                </div>

                <div class="input-field col s12">
                    <input id="registeremail" type="text" class="validate">
                    <label for="email" class="center-align">Email</label>
                </div>
                <div class="input-field col s12">
                    <input id="registerpassword" type="password" class="validate">
                    <label for="password">Password</label>
                </div>
            </div>

            <div class="row">
                <div class="input-field col s12">
                    <a href="#" class="btn waves-effect waves-light col s12 lime" id="register">Create Account</a>
                </div>
            </div>

            <div class="row">
                <div class="input-field col s6 m6 l6">
                    <p class="margin medium-small">Have an account?
                        <a class="signin" id="signin" href="./login.html">Sign In</a>
                    </p>
                </div>
            </div>

        </form>
    </div>
</div>